{
  "name": "codewatch-ui",
  "private": false,
  "version": "0.2.3",
  "description": "Dashboard UI for codewatch",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node ./post_build.js",
    "build-s": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-pd": "npm run build && yalc publish --push",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "error logging",
    "monitoring",
    "dashboard",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gopeeey/codewatch.git",
    "directory": "packages/ui"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "chart.js": "^4.4.4",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "react18-json-view": "^0.2.8",
    "strip-ansi": "^7.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "codewatch-core": "^0.3.0"
  }
}
